<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>
        <%= title %>
    </title>
    <meta name="apple-itunes-app" content="app-id=1016656491">
    <link rel="apple-touch-icon" sizes="57x57" href="<%= protocol %>://<%= host %>/public/favicon/apple-touch-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="<%= protocol %>://<%= host %>/public/favicon/apple-touch-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="<%= protocol %>://<%= host %>/public/favicon/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="<%= protocol %>://<%= host %>/public/favicon/apple-touch-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="<%= protocol %>://<%= host %>/public/favicon/apple-touch-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="<%= protocol %>://<%= host %>/public/favicon/apple-touch-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="<%= protocol %>://<%= host %>/public/favicon/apple-touch-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="<%= protocol %>://<%= host %>/public/favicon/apple-touch-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="<%= protocol %>://<%= host %>/public/favicon/apple-touch-icon-180x180.png">
    <link rel="icon" type="image/png" href="<%= protocol %>://<%= host %>/public/favicon/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="<%= protocol %>://<%= host %>/public/favicon/android-chrome-192x192.png" sizes="192x192">
    <link rel="icon" type="image/png" href="<%= protocol %>://<%= host %>/public/favicon/favicon-96x96.png" sizes="96x96">
    <link rel="icon" type="image/png" href="<%= protocol %>://<%= host %>/public/favicon/favicon-16x16.png" sizes="16x16">
    <link rel="manifest" href="<%= protocol %>://<%= host %>/public/favicon/manifest.json">
    <link rel="mask-icon" href="<%= protocol %>://<%= host %>/public/favicon/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="apple-mobile-web-app-title" content="carswipe.com">
    <meta name="application-name" content="carswipe.com">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="msapplication-TileImage" content="<%= protocol %>://<%= host %>/public/favicon/mstile-144x144.png">
    <meta name="theme-color" content="#ffffff">
    <meta name="description" content="<%= 'Share information on ' + vehicle.maker_name + ' ' + vehicle.model_name + ', vin number '+vehicle.vin_number %>" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Card data -->
    <meta itemprop="name" content="CarSwipe">
    <meta itemprop="description" content="<%= 'Share information on ' + vehicle.maker_name + ' ' + vehicle.model_name + ', vin number '+vehicle.vin_number %>">
    <meta itemprop="image" content="<%= vehicle.image_url[0] %>">

    <!-- Twitter Card data -->
    <meta name="twitter:title" content="CarSwipe">
    <meta name="twitter:card" content="photo">
    <meta name="twitter:site" content="@alexander_zver" data-dynamic="true">
    <meta name="twitter:image" content="<%= vehicle.image_url[0] %>">
    <meta name="twitter:domain" content="carswipe.com">

    <!-- Open Graph data -->
    <meta property="og:title" content="CarSwipe">
    <meta property="og:url" content="<%= protocol %>://<%= host %>/info/<%= vehicle.vin_number %>">
    <meta property="og:image:user_generated" content="1">
    <meta property="og:type" content="article">
    <meta property="og:description" content="<%= 'Share information on ' + vehicle.maker_name + ' ' + vehicle.model_name + ', vin number '+vehicle.vin_number %>">
    <meta property="og:image" content="<%= vehicle.image_url[0] %>">

    <link rel="image_src" href="<%= vehicle.image_url[0] %>" />
    <!-- Bootstrap -->
    <link rel="stylesheet" href="<%= protocol %>://<%= host %>/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="<%= protocol %>://<%= host %>/bootstrap/dist/css/bootstrap-theme.min.css">
    <!-- MY STYLE -->
    <link rel="stylesheet" href="<%= protocol %>://<%= host %>/public/css/styles.css">
    <!-- ANIMATE -->
    <link rel="stylesheet" href="<%= protocol %>://<%= host %>/public/css/animate.css">
</head>

<body class="vin">
    <nav id="topNav" class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand page-scroll" href="<%= protocol %>://<%= host %>">
                    <img id="logo" class="text-center" src="<%= protocol %>://<%= host %>/public/images/logo.png" title="CarSwipe" />
                    <img id="logo-small" class="text-center hidden" src="<%= protocol %>://<%= host %>/public/images/logo-small.png" title="CarSwipe" />
                </a>
            </div>
            <div class="navbar-collapse collapse" id="bs-navbar">
                <ul class="nav navbar-nav navbar-right">
                    <li>
                        <a class="page-scroll" href="<%= protocol %>://<%= host %>">GO TO HOME PAGE</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container" style="margin-top:100px">
        <div class="col-lg-4">
            <div class="row text-center">
                <img class="gallery-preview img-responsive" src="<%= vehicle.image_url[0] %>" />
            </div>
            <% if(vehicle.image_url.length > 1) { %>
                <div class="row text-center" style="margin-top:5px">
                    <% for(var i=0;i<vehicle.image_url.length > 3 ? 3 : vehicle.image_url.length; i++) { %>
                        <a href="#" class="gallery-img"><img class="img-responsive" src="<%= vehicle.image_url[i] %>" /></a>
                        <% } %>
                </div>
                <% } %>
        </div>
        <div class="col-lg-8">
            <h3><%= vehicle.maker_name + ' ' + vehicle.model_name %><span class="pull-right"><%= vehicle.car_year %></span></h3>
        </div>
    </div>
</body>
<!-- Scripts in footer -->
<script src="<%= protocol %>://<%= host %>/jquery/dist/jquery.min.js" type="text/javascript"></script>
<script src="<%= protocol %>://<%= host %>/bootstrap/dist/js/bootstrap.min.js" type="text/javascript"></script>
<script src="<%= protocol %>://<%= host %>/public/js/jquery.easing.min.js" type="text/javascript"></script>
<script src="<%= protocol %>://<%= host %>/public/js/scripts.js" type="text/javascript"></script>
<script type="text/javascript">
    $(".gallery-img").click(function() {
        var img = $(this).find("img")
        if (img) {
            $(".gallery-preview").attr("src", $(img).attr("src"))
        }
        return false;
    })
</script>

</html>