                        <a href="#" class="not-active"><img src="<%= protocol %>://<%= host %>/public/images/image-googleplay.png"></a>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section id="updates">
        <div class="container">
            <div class="row">
                <h2 align="center">Subscribe now for updates!</h2>
            </div>
            <div class="row">
                <form method="POST" action="<%= protocol %>://<%= host %>/subscribe" enctype="application/x-www-form-urlencoded">
                    <div class="col-lg-6 col-md-offset-2 t-r"><input type="email" class="form-control" placeholder="Your Email" name="email" /></div>
                    <div class="col-lg-3 t-l"><input type="submit" class="btn btn-default" value="SUBSCRIBE"></div>
                </form>
            </div>
        </div>
    </section>
